{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning"
            }
        }
    },
    "JWT": {
        "Issuer": "user",
        "Audience": "dsa",
        "Lifetime": 30,
        "SigningKey": "01234567890123456789012345678901"
